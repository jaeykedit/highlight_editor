<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest Version="11.0" ExtensionBundleId="com.example.myceppanel" ExtensionBundleVersion="1.0.0"
    ExtensionBundleName="My CEP Panel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ExtensionList>
        <Extension Id="com.example.myceppanel.panel" Version="1.0.0"/>
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="PPRO" Version="[9.0,99.0]"/>
        </HostList>
        <LocaleList>
            <Locale Code="All"/>
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="11.0"/>
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.example.myceppanel.panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./client/index.html</MainPath>
                    <ScriptPath>./jsx/autoEdit.jsx</ScriptPath>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <MenuName>My CEP Panel</MenuName>
                    <Geometry>
                        <Size>
                            <Height>300</Height>
                            <Width>400</Width>
                        </Size>
                        <MinSize>
                            <Height>200</Height>
                            <Width>200</Width>
                        </MinSize>
                        <MaxSize>
                            <Height>600</Height>
                            <Width>800</Width>
                        </MaxSize>
                    </Geometry>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>